from tkinter import *
from functions import *

class Window(Frame):
    def __init__(self, master=None):
        Frame.__init__(self, master)
        self.master = master

root = Tk()
app = Window(root)

root.wm_title("Pomodoro RPG")
root.geometry("720x480")
root.mainloop()

